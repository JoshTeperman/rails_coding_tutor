<h1>My Students</h1>
<% if @my_students.count.zero? %>
    <h3 >You don't have any students yet.</h3>
    <%= link_to 'Back', :back, class: "btn btn-default btn-rounded mb-4 bg-info text-white" %>
  <% else %>
  <% @my_students.each do |student|%>
    <p><%= student.profile.first_name %> <%= student.profile.surname %></p>
    <%= link_to 'View Profile', profile_path(student.profile.id) %>
  <% end %>
<% end %>